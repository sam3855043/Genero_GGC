                   |col4]\n[col1    |col2       |col3                                  |col4]\n[col1    |col2       |col3                                  |col4]\n[col1    |col2       |col3                                  |col4]\n[col1    |col2       |col3                                  |col4]\n[col1    |col2       |col3                                  |col4]\n[col1    |col2       |col3                                  |col4]\n[col1    |col2       |col3                                  |col4]\n[col1    |col2       |col3                                  |col4]\n}\nEND\nEND\nEND\n\nTABLES\ncustomer, order\nEND\n\nATTRIBUTES\n\nEDIT       f01 = customer.cust_id, NOENTRY;\nEDIT       f02 = customer.cust_name, STYLE = \"important\",\n                 COMMENT = %\"customer.cust_name.comment\";\nLABEL      l03 : TEXT = %\"customer.cust_address.label\";\nEDIT       f03 = customer.cust_address, STYLE=\"address\",\n                 COMMENTS = %\"customer.cust_address.comment\";\nLABEL      l04 : TEXT = %\"customer.cust_zipcode.label\";\nCOMBOBOX   f04 = customer.cust_zipcode,\n                 ITEMS = ((11333,\"ZC11333\"),(22333,\"ZX22333\")),\n                 COMMENTS = %\"customer.cust_zipcode.comment\";\nBUTTONEDIT f05 = customer.cust_state, ACTION=zoom_states,\n                 IMAGE=\"zoom\",\n                 COMMENTS = %\"customer.cust_state.comment\";\nTEXTEDIT   f06 = customer.cust_comment, STRETCH=X,\n                 COMMENTS = %\"customer.cust_comment.comment\";\n\nEDIT col1 = order.ord_id;\nEDIT col2 = order.ord_date;\nEDIT col3 = order.ord_address, SCROLL;\nEDIT col4 = order.ord_custid, HIDDEN;\n\nEND\n\nINSTRUCTIONS\nSCREEN RECORD s_orders (order.*);\nEND\n\n"} {tabIndexRt "1"} {cursor "0"} {cursor2 "0"} {dialogType "Input"} {active "1"}} {{TextEdit 340 {{width "75"} {height "22"} {stretch "both"} {fontPitch "fixed"} {wantReturns "1"} {scroll "1"} {posY "0"} {posX "1"} {gridWidth "75"} {gridHeight "22"} {maxLength "0"}} {}}}}}}}} {Dialog 167 {{active "1"}}